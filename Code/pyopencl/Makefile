.PHONY	: all install clean tags dist userdoc devdoc
# DO NOT EDIT THIS FILE -- it was generated by configure.py
# ./configure.py --python-exe /usr/local/bin/Python

all: tags
	/usr/local/bin/Python setup.py build

dist:
	/usr/local/bin/Python setup.py sdist

install: tags
	/usr/local/bin/Python setup.py install

clean:
	rm -Rf build
	rm -f tags

tags:
	ctags -R src || true

tests:
	echo "running tests"
	find ./test -type f -name "*.py" -exec python {} \;
